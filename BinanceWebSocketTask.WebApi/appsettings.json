{
  "ConnectionStrings": {
    "WebApiDatabase": "Data Source=BinanceWebSocketTaskDatabase"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BinanceStreamsConfiguration": {
    "BaseEndpoint": "wss://stream.binance.com:9443/",
    "WebSocketStreamUriFragment": "stream",
    "WebSocketMultipleStreamsQueryParameter": "?streams=",
    "Symbols": ["btcusdt", "adausdt", "ethusdt"],
    "TimeIntervalBetweenConnectionAttemptsMilliseconds": 10000
  }
}
